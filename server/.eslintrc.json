{
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
        "semi": 2,
        "eqeqeq": "error",
        "no-var": "error"
    },
    "env": {
        "es6": true,
        "node": true,
        "browser": true,
        "jquery": true
    }
}